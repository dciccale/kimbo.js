<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Jasmine Spec Runner</title>
<% css.forEach(function(style){ %>
  <link rel="stylesheet" type="text/css" href="<%= style %>">
<% }) %>
</head>

<body>

  <script type="template" id="fixture-template">
    <section>
      <form>
        <input type="text" name="name" id="name" value="test">
      </form>

      <ul id="list">
        <li class="first active">uno</li>
        <li id="second"><strong>dos</strong></li>
        <li>tres</li>
      </ul>

      <ul id="list2">
        <li>one</li>
        <li>two</li>
        <li>three</li>
      </ul>
    </section>
  </script>

  <div id="fixture-html"></div>

<% with (scripts) { %>
  <% [].concat(polyfills, jasmine, boot, vendor, helpers, src, specs, reporters).forEach(function (script) { %>
    <script src="<%= script %>"></script>
  <% }); %>
<% }; %>

</body>
</html>
